}
.slicedppts{
  display: flex;
  position: relative;
  height: 120rpx;
  background-color: #fff;
}
.cutline{
  position: absolute;
  width: 4rpx;
  height: 96rpx;
  left: 184rpx;
  top: 50%;
  transform: translateY(-50%);
  z-index: 1;
}
.slicedppts-left{
  width: 184rpx;
  text-align: center;
  line-height: 120rpx;
}
.slicedppts-right{
  width: 562rpx;
  text-align:left;
  padding-left: 24rpx;
}
.slicedppt-title{
  font-size: 28rpx;
  color: rgb(53, 53, 53);
  margin: 23rpx 0 10rpx;
}
.slicedppt-speaker{
  font-size: 26rpx;
  color: rgb(136, 136, 136);
}
