.my-ppt-upload{
  background-color: #eee;
  text-align: center;
  height: 658rpx;
}
.my-ppt-upload image{
  width: 442rpx;
  height: 80rpx;
  margin:80rpx 0 68rpx 0;
}
.upload-step{
  text-align: left;
  padding-left: 95rpx;
  background-color: #eee;
}

.upload-tit{
  background-color: #eee;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 26rpx;
  color: rgb(102,102,102);
  margin-bottom: 24rpx;
}
.upload-input,.reget-code{
  display: inline-block;
  width: 424rpx;
  height: 56rpx;
  border-radius: 8rpx;
  line-height: 56rpx;
  padding-left: 16rpx;
  color: rgb(97, 149, 255);
  margin-right: 16rpx;
  border: 2rpx solid rgba(0, 0, 0, .45)
}
.upload-index{
  display: inline-block;
  width: 26rpx;
  height: 26rpx;
  border-radius: 50%;
  background-color: rgb(97, 149, 255);
  line-height: 26rpx;
  color: #fff;
  margin-right: 16rpx;
  text-align: center;
  vertical-align: middle;
}
.reget-code{
  display: inline-block;
  width: 120rpx;
  text-align: center;
  font-size: 26rpx;
  height: 56rpx;
  line-height: 56rpx;
  padding-left:0; 
  color: rgb(53, 53, 53);
}
.upload-tit-text{
  font-size: 26rpx;
   vertical-align: middle;
   color: rgb(53, 53, 53)
}